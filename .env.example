# 자동 생성된 환경변수 템플릿
# 생성일: 2025-05-29

# 공통 설정
# 실행 환경 (선택사항)
NODE_ENV=development

# 로깅 레벨 (선택사항)
LOG_LEVEL=info

# 서버 포트 (선택사항)
PORT=3000

# FIGMA 서비스
# Figma API 접근 토큰
FIGMA_TOKEN=

# Figma 파일 키 (선택사항)
FIGMA_FILE_KEY=

# GITHUB 서비스
# GitHub Personal Access Token
GITHUB_TOKEN=

# GitHub 저장소 소유자
GITHUB_OWNER=

# GitHub 저장소 이름
GITHUB_REPO=

# SUPABASE 서비스
# Supabase 프로젝트 URL
SUPABASE_URL=

# Supabase 익명 키
SUPABASE_ANON_KEY=

# Supabase 서비스 키 (선택사항)
SUPABASE_SERVICE_KEY=

# MCP 서버 포트 설정
FIGMA_MCP_PORT=3001
GITHUB_MCP_PORT=3002
TASKMANAGER_MCP_PORT=3003
SUPABASE_MCP_PORT=3004
DASHBOARD_MCP_PORT=3005
CONTEXT7_MCP_PORT=3006

# Slack/Discord 웹훅 (선택사항)
SLACK_WEBHOOK=
DISCORD_WEBHOOK=

# Vercel 배포 설정 (선택사항)
VERCEL_TOKEN=
VERCEL_ORG_ID=
VERCEL_PROJECT_ID=